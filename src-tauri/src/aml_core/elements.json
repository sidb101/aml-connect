                "default": "ModelVersion.SecondOrder",
                "range_type": "discrete",
                "range": ["ModelVersion.FirstOrder", "ModelVersion.SecondOrder"],
                "unit": null,
                "ui_component": "dropdown"
            }
        }
    },
